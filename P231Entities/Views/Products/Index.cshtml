@model List<Product>;
@{
    ViewBag.Title = "Products";
}
<h1>
    Products page
</h1>

<ul>
    @foreach (Product item in Model)
    {
        <li>
            <div>
                <img width="200" src="@item.Image" alt="@item.Name">
            </div>
            <p style="@(item.Price <100 ? "color:red":"color:black")">
                @item.Name => @item.SKU => @item.Price
            </p>
        </li>
    }
</ul>
